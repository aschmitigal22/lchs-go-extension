{
    "manifest_version": 3,
    "name": "LCHS Go (beta)",
    "version": "1.0.0",
    "description": "Extension version of betago.lciteam.club.",
    "icons": {
    },
    "action": {
        "default_title": "LCHS Go (beta)",
        "default_popup": "popup.html",
        "default_icon": {
        }
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "content_scripts": [
        {
            "matches": ["http://127.0.0.1:5500/*"],
            "js": ["extractData.js"]
        }
    ],
    "homepage_url": "http://127.0.0.1:5500",
    "offline_enabled": true,
    "permissions": [
        "idle",
        "storage"
    ]
}